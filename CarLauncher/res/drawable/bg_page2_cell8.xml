<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <layer-list>
            <item>
                <bitmap android:src="@drawable/bg_cell28"
                        android:gravity="center"/>
            </item>
            <item>
                <shape  android:shape="rectangle">
                    <solid android:color="@color/press_mask"/>
                </shape>
            </item>
        </layer-list>
    </item>
    <item android:drawable="@drawable/bg_cell28" android:state_pressed="false"/>
</selector>
